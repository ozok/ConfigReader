@using WebEditor.ViewModels;
@model IEnumerable<ConfigurationListeItemViewModel>
@{
    ViewData["Title"] = "Configurations";
}

<div >

    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Value</th>
                <th scope="col">Application</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Id
                    </td>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Value
                    </td>
                    <td>
                        @item.ApplicationName
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
